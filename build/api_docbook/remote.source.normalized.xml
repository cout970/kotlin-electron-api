<?xml version="1.0" encoding="UTF-8"?>
<file>
   <object title="remote">
      <methods>
         <method name="require">
            <param name="module" type="String"/>
            <returns type="any">
               <description>
                  <para>The object returned by require(module) in the main process.</para>
               </description>
            </returns>
         </method>
         <method name="getCurrentWindow">
            <returns type="BrowserWindow">
               <description>
                  <para>The window to which this web page belongs.</para>
               </description>
            </returns>
         </method>
         <method name="getCurrentWebContents">
            <returns type="WebContents">
               <description>
                  <para>The web contents of this web page.</para>
               </description>
            </returns>
         </method>
         <method name="getGlobal">
            <param name="name" type="String"/>
            <returns type="any">
               <description>
                  <para>The global variable of name (e.g. global[name]) in the main process.</para>
               </description>
            </returns>
         </method>
      </methods>
   </object>
</file>
