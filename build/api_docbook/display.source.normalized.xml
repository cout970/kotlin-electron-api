<?xml version="1.0" encoding="UTF-8"?>
<file>
   <struct title="Display">
      <property name="id" type="Number">
         <description>
            <para>Unique identifier associated with the display.</para>
         </description>
      </property>
      <property name="rotation" type="Number">
         <description>
            <para>Can be 0, 90, 180, 270, represents screen rotation in clock-wise degrees.</para>
         </description>
      </property>
      <property name="scaleFactor" type="Number">
         <description>
            <para>Output device's pixel scale factor.</para>
         </description>
      </property>
      <property name="touchSupport" type="String">
         <description>
            <para>Can be available, unavailable, unknown.</para>
         </description>
      </property>
      <property name="bounds" type="Rectangle"/>
      <property name="size" type="Object">
         <property name="height" type="Number"/>
         <property name="width" type="Number"/>
      </property>
      <property name="workArea" type="Rectangle"/>
      <property name="workAreaSize" type="Object">
         <property name="height" type="Number"/>
         <property name="width" type="Number"/>
      </property>
   </struct>
</file>
